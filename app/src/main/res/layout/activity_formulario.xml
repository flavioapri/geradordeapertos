<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    tools:context=".Formulario">

    <EditText
        android:id="@+id/et_np"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="118dp"
        android:autofillHints=""
        android:gravity="center"
        android:inputType="number"
        android:maxLength="9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_np"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_np"
        android:layout_width="27dp"
        android:layout_height="45dp"
        android:layout_marginStart="117dp"
        android:fontFamily="@font/product_sans"
        android:gravity="center_vertical"
        android:text="@string/tv_np"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_processo"
        android:layout_width="84dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/product_sans"
        android:gravity="center_vertical"
        android:text="@string/tv_processo"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_np" />

    <Spinner
        android:id="@+id/sp_processo"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_processo"
        app:layout_constraintTop_toBottomOf="@+id/et_np" />

    <TextView
        android:id="@+id/tv_historico_programa"
        android:layout_width="87dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/product_sans"
        android:gravity="center_vertical"
        android:text="@string/tv_programa"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_processo" />

    <Spinner
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_historico_programa"
        app:layout_constraintTop_toBottomOf="@+id/sp_processo" />

    <TextView
        android:id="@+id/tv_ciclo"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/product_sans"
        android:gravity="center_vertical"
        android:text="@string/tv_ciclo"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_historico_programa" />

    <CheckBox
        android:id="@+id/cb_ciclo_1"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_1"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_ciclo" />

    <CheckBox
        android:id="@+id/cb_ciclo_2"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_2"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_1"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_1" />

    <CheckBox
        android:id="@+id/cb_ciclo_3"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_3"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_2"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_1" />

    <CheckBox
        android:id="@+id/cb_ciclo_4"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_4"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_3"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_1" />

    <CheckBox
        android:id="@+id/cb_ciclo_5"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_5"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_4"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_1" />

    <CheckBox
        android:id="@+id/cb_ciclo_6"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_6"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_5"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_1" />

    <CheckBox
        android:id="@+id/cb_ciclo_7"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_7"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cb_ciclo_1" />

    <CheckBox
        android:id="@+id/cb_ciclo_8"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_8"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_7"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_7" />

    <CheckBox
        android:id="@+id/cb_ciclo_9"
        android:layout_width="52dp"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_9"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_8"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_7" />

    <CheckBox
        android:id="@+id/cb_ciclo_10"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_10"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_9"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_7" />

    <CheckBox
        android:id="@+id/cb_ciclo_11"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_11"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/cb_ciclo_12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_10"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_7" />

    <CheckBox
        android:id="@+id/cb_ciclo_12"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:fontFamily="@font/product_sans"
        android:text="@string/cb_ciclo_12"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cb_ciclo_11"
        app:layout_constraintTop_toTopOf="@id/cb_ciclo_7" />

    <TextView
        android:id="@+id/tv_motivo"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/product_sans"
        android:gravity="center_vertical"
        android:text="@string/tv_motivo"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cb_ciclo_7" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_motivo"
        app:layout_constraintTop_toBottomOf="@+id/cb_ciclo_8" />


    <TextView
        android:id="@+id/tv_data"
        android:layout_width="42dp"
        android:layout_height="45dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/product_sans"
        android:gravity="center_vertical"
        android:text="@string/tv_data"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner" />

    <EditText
        android:layout_width="122dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="100dp"
        android:autofillHints=""
        android:ems="10"
        android:gravity="center"
        android:inputType="date"
        android:maxLength="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_data"
        app:layout_constraintTop_toBottomOf="@+id/spinner" />

    <Button
        android:id="@+id/bt_home"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/bt_principal"
        android:elevation="8dp"
        android:fontFamily="@font/font_awesome_5"
        android:gravity="center"
        android:stateListAnimator="@null"
        android:text="@string/icon_home"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bt_scan"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/bt_scan"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:background="@drawable/bt_principal"
        android:elevation="8dp"
        android:fontFamily="@font/font_awesome_5"
        android:gravity="center"
        android:stateListAnimator="@null"
        android:text="@string/icon_scanner"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/bt_confirm"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/bt_home"
        app:layout_constraintTop_toTopOf="@+id/bt_home" />

    <Button
        android:id="@+id/bt_confirm"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:background="@drawable/bt_principal"
        android:elevation="8dp"
        android:fontFamily="@font/font_awesome_5"
        android:gravity="center"
        android:stateListAnimator="@null"
        android:text="@string/icon_confirm"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/bt_clear"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/bt_scan"
        app:layout_constraintTop_toTopOf="@+id/bt_scan" />

    <Button
        android:id="@+id/bt_clear"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:background="@drawable/bt_principal"
        android:elevation="8dp"
        android:fontFamily="@font/font_awesome_5"
        android:gravity="center"
        android:stateListAnimator="@null"
        android:text="@string/icon_clear"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/bt_confirm"
        app:layout_constraintTop_toTopOf="@+id/bt_confirm" />

</androidx.constraintlayout.widget.ConstraintLayout>